<div class="jumbotron">
  <h1>$title.getTitle()</h1>
</div>
<ol>
#foreach($meaning in $uses)
  <li>$meaning.getMeaning()</li>
#end
</ol>


<form action="/words/$title.getId()/meaning" method="post">
 <label for="meaning">Definition:</label>
  <input id="meaning" name="meaning" type="text">

  <button type="submit" class="btn btn-success">Add Definition</button>
</form>

<a href="/">Back to All Words</a>
